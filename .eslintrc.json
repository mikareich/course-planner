{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "import", "jsx-a11y", "react-hooks"],
  "rules": {
    "react/prop-types": 0,
    "react-hooks/rules-of-hooks": "error",
    "no-console": "warn",
    "react/jsx-filename-extension": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
